        "laminas/laminas-diactoros": "^2.8.0",
        "imagine/imagine": "~0.5.0",
        "ps/image-optimizer": "^2.0.5",
        "illuminate/database": "^8.83",
        "illuminate/redis": "^8.83",
        "illuminate/validation": "^8.83",
